{
  "name": "waveconnect",
  "version": "1.0.0",
  "main": "dist/app.js",
  "type": "module",
  "scripts": {
    "start": "node dist/app.js",
    "dev": "ts-node --esm src/app.ts",
    "build": "npx prisma generate && tsc",
    "vercel-build": "npx prisma generate && npm run build",
    "postinstall": "prisma generate",
    "migrate": "npx prisma migrate deploy",
    "clean": "rm -rf dist"
  },
  "engines": {
    "node": ">=18.x"
  },
  "keywords": [
    "wave",
    "connect",
    "express",
    "typescript"
  ],
  "author": "Ibrahima ISIDev",
  "license": "ISC",
  "description": "A TypeScript project for wave connection using Express and Prisma.",
  "dependencies": {
    "@prisma/client": "^5.21.1",
    "bcrypt": "^5.1.1",
    "cloudinary": "^2.5.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.21.1",
    "http": "^0.0.1-security",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "qrcode": "^1.5.4",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1",
    "twilio": "^5.3.5",
    "typescript": "^5.6.3",
    "@types/node": "^18.19.61",
    "prisma": "^5.21.1"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.17",
    "@types/dotenv": "^6.1.1",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/multer": "^1.4.12",
    "@types/qrcode": "^1.5.5",
    "@types/socket.io": "^3.0.1",
    "nodemon": "^3.1.7",
    "ts-node": "^10.9.1"
  }
}